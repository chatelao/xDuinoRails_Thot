      <lightbulb id="L1" x="0" y="10" color="green"/>
      <lightbulb id="L2" x="0" y="0" color="red"/>
      <lightbulb id="L3" x="0" y="-10" color="yellow"/>
    </lightbulbs>
    <aspects>
      <aspect name="Stop">
        <light id="L2"/>
      </aspect>
      <aspect name="Proceed">
        <light id="L1"/>
      </aspect>
      <aspect name="Proceed at reduced speed">
        <light id="L1"/>
        <light id="L3"/>
      </aspect>
    </aspects>
  </signal>
